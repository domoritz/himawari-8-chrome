{
  "manifest_version": 2,
  "name": "Himawari 8 New Tab Page",
  "short_name": "Himawari 8",
  "description": "Experience the latest image from the Himawari 8 satellite every time you open a new tab.",
  "version": "0.3.2",
  "homepage_url": "https://github.com/domoritz/himawari-8-chrome",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "minimum_chrome_version": "42",
  "offline_enabled": true,
  "icons": {
    "128": "icons/icon128.png",
    "48":"icons/icon48.png",
    "16":"icons/icon16.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}